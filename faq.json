<!-- 临时测试页面 test.html -->
<!DOCTYPE html>
<html>
<head>
    <title>Test JSON</title>
</head>
<body>
    <h1>Testing faq.json</h1>
    <div id="result"></div>
    
    <script>
        fetch('faq.json')
            .then(response => response.text())
            .then(text => {
                document.getElementById('result').innerHTML = 
                    '<h2>Raw Content:</h2><pre>' + text + '</pre>' +
                    '<h2>Parsed:</h2><pre>' + JSON.stringify(JSON.parse(text), null, 2) + '</pre>';
            })
            .catch(error => {
                document.getElementById('result').innerHTML = 
                    '<h2>Error:</h2><pre>' + error + '</pre>';
            });
    </script>
</body>
</html>
